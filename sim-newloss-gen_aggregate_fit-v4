     "iopub.execute_input": "2024-05-21T05:38:58.435123Z",
     "iopub.status.busy": "2024-05-21T05:38:58.434762Z",
     "iopub.status.idle": "2024-05-21T05:38:58.444202Z",
     "shell.execute_reply": "2024-05-21T05:38:58.443090Z"
