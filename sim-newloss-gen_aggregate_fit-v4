    "criterion_ = nn.NLLLoss()\n",
    "def discriminator_loss(pred_labels, real_labels):\n",
    "    log_probs = torch.log(pred_labels)\n",
    "    loss = criterion_(log_probs, real_labels)\n",
    "    return loss"
   ]
