    "def gen_results(directory):\n",
    "    gen_results = []\n",
    "    for filename in os.listdir(directory):\n",
    "        filepath = os.path.join(directory, filename)\n",
    "        gen_results.append((torch.load(filepath)[\"state_dict\"] , torch.load(filepath)[\"num_examples\"]))\n",
    "    !rm -rf directory/*\n",
    "    return gen_results "
